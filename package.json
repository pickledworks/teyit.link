  "description": "",
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "start:dev": "babel-node ./index.js",
    "test": "xo",
    "build": "rm -rf dist/ && babel --ignore public,node_modules -d dist/ .",
    "precommit": "npm test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.28.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "express-hbs": "^1.0.4",
    "express-session": "^1.15.2",
    "lusca": "^1.4.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.1",
    "mysql": "^2.13.0",
    "randomstring": "^1.1.5",
    "ratelimit.js": "^1.8.0",
    "redis": "^2.7.1",
    "sequelize": "^3.30.4",
    "serve-favicon": "^2.4.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "husky": "^0.13.3",
    "xo": "^0.18.1"
  },
  "babel": {
    "plugins": [],
    "presets": [
      "es2015",
      "env"
    ]
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "envs": [],
    "global": [],
    "rules": {
      "array-callback-return": 0,
      "brace-style": [
        "error",
        "stroustrup",
        {
          "allowSingleLine": true
        }
      ],
      "camelcase": 0,
      "capitalized-comments": 0,
      "max-params": 0,
      "new-cap": 0,
      "no-unused-vars": 0,
      "unicorn/no-process-exit": 0
    },
    "ignore": [
      "dist/*",
      "public/*"
    ],
    "overrides": [],
    "extends": [],
    "plugins": []
  }
}
